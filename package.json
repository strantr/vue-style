{
  "name": "vue-style",
  "version": "1.0.0-alpha.2",
  "description": "Vue.js style component allowing for application theming via CSS variable syntax",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "prepublish": "npm run build && npm run test",
    "pretest": "webpack --config test/webpack.config.js",
    "test": "mocha test/bundle.js --require chai --require jsdom-global/register",
    "dev": "webpack-dev-server --config webpack.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strantr/vue-style.git"
  },
  "keywords": [
    "vue",
    "vue.js",
    "style",
    "theme",
    "component"
  ],
  "author": "strantr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strantr/vue-style/issues"
  },
  "homepage": "https://github.com/strantr/vue-style#readme",
  "peerDependencies": {
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/mocha": "^2.2.31",
    "@types/node": "^7.0.8",
    "chai": "^3.5.0",
    "css-loader": "^0.27.1",
    "html-webpack-plugin": "^2.28.0",
    "in-publish": "^2.0.0",
    "jsdom": "9.11.0",
    "jsdom-global": "2.1.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "vue": "^2.3.3",
    "vuety": "^2.1.0-beta.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts"
}